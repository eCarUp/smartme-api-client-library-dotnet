@page
@model OAuthWebExample.Pages.MBus.SendTelegramModel
@{
    ViewData["Title"] = "MBus";
}

<h2>Send MBus Telegram</h2>

<div class="row">
    <div class="col-md-12">
        @if (User.Identity.IsAuthenticated)
        {
        <form method="post">
            <button type="submit" class="btn btn-default">Send</button>
        </form>
        }
    </div>
</div>